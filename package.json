{
	"name": "@fourlights/mapper-plugin-anonymize",
	"version": "1.3.0",
	"description": "Fuzzy anonymization plugin for @fourlights/mapper",
	"packageManager": "npm@10.5.0",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"release": "release-it",
		"prepare": "node .husky/install.mjs",
		"build": "tsup",
		"lint": "oxlint",
		"lint:fix": "oxlint --fix",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"test": "jest",
		"test:fast": "TEST_FAST=true jest",
		"packages:upgrade": "npx --yes npm-check-updates -u"
	},
	"author": "Four Lights <engineering@fourlights.nl> (https://github.com/Four-Lights-NL)",
	"maintainers": [
		"Thomas Rijpstra - Four Lights <thomas@fourlights.nl> (https://github.com/trijpstra-fourlights)"
	],
	"homepage": "https://github.com/Four-Lights-NL/mapper-plugin-anonymize#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Four-Lights-NL/mapper-plugin-anonymize.git"
	},
	"bugs": {
		"url": "https://github.com/Four-Lights-NL/mapper-plugin-anonymize/issues"
	},
	"license": "MIT",
	"keywords": [
		"plugin",
		"mapper",
		"anonymize",
		"anonymization",
		"faker",
		"redact",
		"redaction"
	],
	"peerDependencies": {
		"@fourlights/mapper": "^1.3.0"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^19.2.2",
		"@fourlights/mapper": "^1.3.0",
		"@release-it/conventional-changelog": "^8.0.1",
		"@types/jest": "^29.5.12",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"oxlint": "^0.3.1",
		"prettier": "^3.2.5",
		"release-it": "^17.2.0",
		"simple-git": "^3.24.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@faker-js/faker": "^8.4.1",
		"@leeoniya/ufuzzy": "^1.0.14",
		"defu": "^6.1.4",
		"set-value": "^4.1.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
